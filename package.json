{
  "name": "linter-jsonlint",
  "version": "1.1.4",
  "private": true,
  "description": "A plugin for Atom Linter providing an interface to jsonlint",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/AtomLinter/linter-jsonlint"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "test": "eslint ."
  },
  "keywords": [
    "json",
    "lint",
    "jsonlint"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtomLinter/linter-jsonlint/issues"
  },
  "homepage": "https://github.com/AtomLinter/linter-jsonlint#readme",
  "dependencies": {
    "atom-linter": "^4.3.1",
    "atom-package-deps": "^3.0.7",
    "jsonlint": "~1.6.2"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0"
  },
  "eslintConfig": {
    "rules": {
      "comma-dangle": [
        2,
        "never"
      ],
      "no-console": 0
    },
    "extends": "airbnb/base",
    "parser": "babel-eslint",
    "globals": {
      "atom": true
    },
    "env": {
      "es6": true,
      "node": true
    }
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  }
}
