{
  "name": "linter-jsonlint",
  "version": "1.3.0",
  "private": true,
  "description": "A plugin for Atom Linter providing an interface to jsonlint",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/AtomLinter/linter-jsonlint"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "test": "apm test",
    "lint": "eslint ."
  },
  "keywords": [
    "json",
    "lint",
    "jsonlint"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtomLinter/linter-jsonlint/issues"
  },
  "homepage": "https://github.com/AtomLinter/linter-jsonlint#readme",
  "dependencies": {
    "atom-package-deps": "^4.0.1",
    "jsonlint": "~1.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "@commitlint/travis-cli": "^6.1.3",
    "@semantic-release/apm-config": "^2.0.1",
    "husky": "^0.14.3",
    "semantic-release": "^15.1.7",
    "eslint": "^4.6.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0"
  },
  "eslintConfig": {
    "rules": {
      "comma-dangle": [
        "error",
        "never"
      ],
      "global-require": "off",
      "import/no-unresolved": [
        "error",
        {
          "ignore": [
            "atom"
          ]
        }
      ]
    },
    "extends": "airbnb-base",
    "globals": {
      "atom": true
    },
    "env": {
      "node": true
    }
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "release": {
    "extends": "@semantic-release/apm-config"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}